{
    "name": "ryvova/linkedlist",
    "description": "SortedLinkedListPackage",
    "autoload": {
        "psr-4": {
            "LinkedList\\": "src/",
            "LinkedList\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "phpunit": "php.exe C:/xampp/htdocs/LinkedList/vendor/phpunit/phpunit/phpunit tests",
        "stan": "vendor/bin/phpstan analyse src tests --level 9"
    },
    "require": {
        "php": ">=8.2",
        "ext-intl": "*"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^9"
    }
}
